findErrorChar : String -> Maybe Char
findErrorChar line =
    line
        |> String.toList
        |> parseLine (Incomplete [])
        |> errorFromParseResult


errorFromParseResult : ParseResult -> Maybe Char
errorFromParseResult a =
    case a of
        Error x ->
            Just x

        _ ->
            Nothing


type ParseResult
    = Incomplete (List Char)
    | Error Char
    | Complete


parseLine : ParseResult -> List Char -> ParseResult
parseLine result inputChars =
    case result of
        Complete ->
            Complete

        Error char ->
            Error char

        Incomplete tracker ->
            case inputChars of
                [] ->
                    Incomplete tracker

                head :: tail ->
                    if isOpeningTag head then
                        parseLine (Incomplete (unsafeGetClosingTag head :: tracker)) tail

                    else
                        case tracker of
                            [] ->
                                Debug.todo "nothing to close"

                            thead :: ttail ->
                                if thead == head then
                                    parseLine (Incomplete ttail) tail

                                else
                                    parseLine (Error head) tail


isOpeningTag : Char -> Bool
isOpeningTag char =
    List.member char [ '(', '{', '<', '[' ]


unsafeGetClosingTag : Char -> Char
unsafeGetClosingTag char =
    unsafeGet char closingMap


unsafeGet : comparable -> Dict comparable a -> a
unsafeGet key dict =
    case Dict.get key dict of
        Just x ->
            x

        Nothing ->
            Debug.todo "key not found in dict"


computeErrorScore : Char -> Int -> Int
computeErrorScore char total =
    total + unsafeGet char scoreMap


run2 =
    actualInput
        |> String.lines
        |> List.filterMap filterIncompleteLines
        |> List.map computeIncompleteScoreForLine
        |> getMiddleScore


filterIncompleteLines : String -> Maybe (List Char)
filterIncompleteLines line =
    line
        |> String.toList
        |> parseLine (Incomplete [])
        |> incompleteFromParseResult
        |> Just
        |> Maybe.andThen
            (\l ->
                if List.length l == 0 then
                    Nothing

                else
                    Just l
            )


incompleteFromParseResult : ParseResult -> List Char
incompleteFromParseResult r =
    case r of
        Incomplete list ->
            list

        _ ->
            []


computeIncompleteScoreForLine : List Char -> Int
computeIncompleteScoreForLine =
    List.foldl
        (\char total -> (total * 5) + unsafeGet char incompleteScoreMap)
        0


incompleteScoreMap =
    Dict.fromList
        [ ( ')', 1 )
        , ( ']', 2 )
        , ( '}', 3 )
        , ( '>', 4 )
        ]


getMiddleScore : List Int -> Maybe Int
getMiddleScore list =
    list
        |> List.sort
        |> List.Extra.getAt (List.length list // 2)
